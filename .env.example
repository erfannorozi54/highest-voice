NEXT_PUBLIC_PROJECT_ID=

# Hardhat Deployment Secrets
SEPOLIA_RPC_URL=



# Network Configuration
# Set NETWORK to 'local', 'sepolia', or 'mainnet'
NETWORK=local

# Infura Project ID (required for Sepolia and Mainnet)
# Get one from https://infura.io/
INFURA_PROJECT_ID=

# Private Key (required for Sepolia and Mainnet deployments)
# NEVER commit this to version control!
PRIVATE_KEY=

# Optional: Custom RPC URLs (if not using Infura)
SEPOLIA_RPC_URL=
MAINNET_RPC_URL=

# Etherscan API Key (optional, for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Protocol Guild address for testnet (optional, defaults to deployer)
# Used for treasury distribution on Sepolia
TEST_PROTOCOL_GUILD=

# Gas reporting (optional)
REPORT_GAS=false

# Frontend Configuration (automatically set by deploy script)
NEXT_PUBLIC_PROJECT_ID=
NEXT_PUBLIC_HIGHEST_VOICE_CONTRACT=
NEXT_PUBLIC_NETWORK=local
NEXT_PUBLIC_CHAIN_ID=31337
NEXT_PUBLIC_RPC_URL=http://127.0.0.1:8545
